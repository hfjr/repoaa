    <article class="PublicArticle">
        <section class="zibenye">
            <h2 class="FloatLeft">关于华夏银行
</h2>
            <div class="FloatRight">
                <img src="<c:url value='/static/h5/img/huaxiayh.png'/>" style="margin:5px 0 0 23px;">
            </div>
              <div class="cbdiv"></div>
