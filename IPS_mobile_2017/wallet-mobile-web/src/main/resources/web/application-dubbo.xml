<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
    http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

	<!-- 注册名 -->
	<dubbo:application name="wallet-mobile-web" />

	<!-- zk服务地址 -->
	<dubbo:registry address="${zookeeper.address}" />

	<!-- 服务端口 -->
	<dubbo:protocol name="dubbo" port="${dubbo.port}" />

    <dubbo:consumer check="false" timeout="${dubbo.consumer.timeout}" />

    <dubbo:provider timeout="${dubbo.provider.timeout}" retries="${dubbo.provider.retries}"/>
	<!-- 注解dubbo服务 -->
	<dubbo:annotation package="com.zhuanyi.vjwealth.wallet.mobile.credit" />

</beans>